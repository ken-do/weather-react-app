{
  "name": "weather-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "6.1.1",
    "@loadable/babel-plugin": "5.13.2",
    "@loadable/component": "5.14.1",
    "@loadable/server": "5.14.2",
    "@reduxjs/toolkit": "1.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "add": "2.0.6",
    "app-root-path": "3.0.0",
    "axios": "0.21.1",
    "bootstrap": "4.6.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "lodash": "4.17.20",
    "morgan": "1.10.0",
    "path": "0.12.7",
    "react": "^17.0.1",
    "react-bootstrap": "1.4.3",
    "react-dom": "^17.0.1",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.2",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
    "snapshot-diff": "0.8.1",
    "typescript": "^4.1.2",
    "url-loader": "4.1.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "dev:app": "craco start",
    "dev:api": "cross-env NODE_ENV=development nodemon --project tsconfig.server.json --watch src/server --ext 'ts,json'  --exec ts-node src/server/index.ts",
    "dev": "run-p dev:*",
    "build": "craco build",
    "build:ssr": "cross-env NODE_ENV=production webpack --config webpack.server.js",
    "start:ssr": "node build-server/index.js",
    "start:ssr:bg": "pm2 start build-server/index.js --name weather-react-app",
    "stop:ssr:bg": "pm2 stop weather-react-app",
    "start": "run-s build build:ssr start:ssr",
    "test": "craco test",
    "test:coverage": "yarn test --collectCoverage --watchAll",
    "e2e": "yarn cypress run",
    "e2e:open": "yarn cypress open",
    "eject": "react-scripts eject",
    "preinstall": "node ./scripts/checkYarn.js",
    "postinstall": "husky install",
    "format": "yarn prettier --write src",
    "lint:script": "eslint --fix --resolve-plugins-relative-to .",
    "lint:style": "stylelint --fix",
    "lint:all:script": "yarn lint:script  --ext ts,tsx .",
    "lint:all:style": "yarn lint:style \"src/**/*.{css,scss}\"",
    "lint:all:filename": "ls-lint",
    "lint:all": "run-p lint:all:*"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --resolve-plugins-relative-to ."
    ],
    "*.?(s)css": [
      "prettier --write",
      "yarn lint:all:style"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@loadable/webpack-plugin": "5.14.2",
    "@ls-lint/ls-lint": "1.9.2",
    "@types/app-root-path": "1.2.4",
    "@types/cors": "2.8.9",
    "@types/express": "4.17.11",
    "@types/loadable__component": "5.13.2",
    "@types/loadable__server": "5.12.3",
    "@types/lodash": "4.14.168",
    "@types/morgan": "1.9.2",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/redux": "3.6.0",
    "@types/redux-logger": "3.0.8",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "cross-env": "7.0.3",
    "css-loader": "5.0.2",
    "cypress": "6.4.0",
    "dotenv-webpack": "6.0.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "6.2.0",
    "husky": "5.0.8",
    "lint-staged": "10.5.4",
    "loadable-ts-transformer": "1.0.0-alpha.3",
    "mini-css-extract-plugin": "1.3.6",
    "node-sass": "5.0.0",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "pm2": "4.5.4",
    "prettier": "2.2.1",
    "stylelint": "13.9.0",
    "stylelint-config-airbnb": "0.0.0",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recess-order": "2.3.0",
    "stylelint-scss": "3.18.0",
    "ts-loader": "8.0.17",
    "ts-node": "9.1.1",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack-cli": "4.5.0",
    "webpack-merge": "5.7.3",
    "webpack-node-externals": "2.5.2"
  },
  "resolutions": {
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-react": "7.22.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!node_modules/",
      "!src/reportWebVitals.ts",
      "!src/setupTests.ts",
      "!src/**/index.{ts,tsx}",
      "!src/types/*.ts",
      "!src/server/**/*.ts",
      "!src/utils/weatherForecastImages.ts"
    ]
  }
}